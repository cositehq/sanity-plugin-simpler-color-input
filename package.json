    "lint-staged": "^13.2.2",
    "npm-run-all2": "^5.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.4.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.1",
    "sanity": "^3.57.3",
    "semantic-release": "^24.1.1",
    "styled-components": "^6.1",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "react": "^18.3",
    "sanity": "^3.23.0",
    "styled-components": "^6.1"
  },
  "engines": {
    "node": ">=14"
