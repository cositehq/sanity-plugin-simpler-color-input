{
  "name": "sanity-plugin-simpler-color-input",
  "version": "2.2.0",
  "description": "A simpler color input",
  "keywords": [
    "sanity",
    "sanity-plugin",
    "color-input"
  ],
  "homepage": "https://github.com/cositehq/sanity-plugin-simpler-color-input#readme",
  "bugs": {
    "url": "https://github.com/cositehq/sanity-plugin-simpler-color-input/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cositehq/sanity-plugin-simpler-color-input.git"
  },
  "license": "MIT",
  "author": "Alyssa Melendez <alyssa@cositehq.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sanity/icons": "^2.3.1",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^1.3.3",
    "@uiw/react-color": "^2.0.8",
    "react-icons": "^4.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@sanity/pkg-utils": "^2.2.16",
    "@sanity/plugin-kit": "^3.1.7",
    "@sanity/semantic-release-preset": "^4.1.1",
    "@types/react": "^18.2.7",
    "@types/react-color": "^2.17.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.1",
    "sanity": "^3.11.3",
    "styled-components": "^5.3.11",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "@sanity/ui": "^1.0 || ^2.0.0-beta",
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=14"
  }
}
